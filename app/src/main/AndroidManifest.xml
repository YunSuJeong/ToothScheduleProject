<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toothscheduleproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToothScheduleProject">

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SignUp">

        </activity>

        <!--    sub액티비티 등록    -->
        <activity android:name=".Notification"/>
        <activity android:name=".InfoBoard"/>
        <activity android:name=".Survey"/>
        <activity android:name=".SurveyResult"/>
        <activity android:name=".SurveyResultBad"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".Schedule"/>
        <activity android:name=".ScheduleDetail"/>
        <activity android:name=".MyPage"/>

    </application>

</manifest>
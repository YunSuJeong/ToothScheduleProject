<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toothscheduleproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToothScheduleProject">

        <activity android:name=".LoginPage.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginPage.SignUp">

        </activity>

        <!--    sub액티비티 등록    -->
        <activity android:name=".Notification.Notification"/>
        <activity android:name=".InfoBoard.Infoboard"/>
        <activity android:name=".Survey.SurveyDo"/>
        <activity android:name=".Survey.SurveyResult"/>
        <activity android:name=".Survey.SurveyResultBad"/>
        <activity android:name=".Main.MainActivity" />
        <activity android:name=".Main.MyPage"/>
        <activity android:name=".ScheduleSetting.Schedule"/>
        <activity android:name=".ScheduleSetting.ScheduleDetail"/>
        <receiver android:name=".Notification.AlarmReceiver"></receiver>
    </application>

</manifest>